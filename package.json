{
  "name": "@nrfcloud/bdd-feature-runner-aws",
  "version": "0.0.0-development",
  "description": "BDD Test Runner for cloud native applications made with AWS",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepare": "tsc",
    "lint": "npm run tslint && npm run prettier:lint",
    "tslint": "tslint --project ./tsconfig.json",
    "prettier": "prettier --write \"./src/**/*.ts\"",
    "prettier:lint": "prettier --list-different \"./src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nRFCloud/bdd-feature-runner-aws.git"
  },
  "keywords": [
    "nRFCloud",
    "IoT",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/nRFCloud/bdd-feature-runner-aws/issues"
  },
  "homepage": "https://github.com/nRFCloud/bdd-feature-runner-aws#readme",
  "dependencies": {
    "@types/backoff": "^2.5.1",
    "@types/chai": "^4.1.7",
    "@types/glob": "^7.1.1",
    "@types/jsonata": "^1.3.0",
    "aws-sdk": "^2.374.0",
    "backoff": "^2.5.0",
    "chai": "^4.2.0",
    "gherkin": "^5.1.0",
    "glob": "^7.1.3",
    "jsonata": "^1.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-angular": "^7.1.2",
    "@nrfcloud/tslint-config": "^3.7.0",
    "@types/node": "^10.12.2",
    "husky": "^1.1.3",
    "prettier": "^1.14.3",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "files": [
    "package-lock.json",
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "saga",
    "remoteTags": true
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "npm run lint"
    }
  }
}
