{
  "name": "@coderbyheart/bdd-feature-runner-aws",
  "version": "0.0.0-development",
  "description": "BDD Test Runner for cloud native applications made with AWS",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepare": "tsc",
    "lint": "npm run eslint && npm run prettier:lint",
    "eslint": "eslint  --ext .js,.ts \"./src/**/*.ts\"",
    "prettier": "prettier --write \"./src/**/*.ts\"",
    "prettier:lint": "prettier --list-different \"./src/**/*.ts\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderbyheart/bdd-feature-runner-aws.git"
  },
  "keywords": [
    "BDD",
    "end-to-end",
    "testing",
    "serverless",
    "cloud native",
    "aws"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/coderbyheart/bdd-feature-runner-aws/issues"
  },
  "homepage": "https://github.com/coderbyheart/bdd-feature-runner-aws#readme",
  "dependencies": {
    "@types/backoff": "^2.5.1",
    "@types/chai": "^4.2.11",
    "@types/glob": "^7.1.1",
    "@types/jsonata": "^1.5.1",
    "@types/node-fetch": "^2.5.5",
    "aws-sdk": "^2.656.0",
    "backoff": "^2.5.0",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "chalk": "^4.0.0",
    "fetch-ponyfill": "^6.1.0",
    "gherkin": "^9.0.0",
    "glob": "^7.1.6",
    "graphql": "^15.0.0",
    "jsonata": "^1.8.2",
    "node-fetch": "^2.6.0",
    "paho-mqtt": "^1.1.0",
    "toposort": "^2.0.2",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@bifravst/code-style": "^7.0.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@types/chai-subset": "^1.3.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.8",
    "@types/paho-mqtt": "^1.0.4",
    "@types/toposort": "^2.0.3",
    "@types/ws": "^7.2.3",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "package-lock.json",
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=5.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged && npx jest --onlyChanged"
    }
  },
  "lint-staged": {
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".+\\.spec\\.ts$",
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  }
}
