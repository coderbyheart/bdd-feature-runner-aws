{
  "name": "@coderbyheart/bdd-feature-runner-aws",
  "version": "0.0.0-development",
  "description": "BDD Test Runner for cloud native applications made with AWS",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepare": "tsc",
    "lint": "npm run eslint && npm run prettier:lint",
    "eslint": "eslint  --ext .js,.ts \"./src/**/*.ts\"",
    "prettier": "prettier --write \"./src/**/*.ts\"",
    "prettier:lint": "prettier --list-different \"./src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderbyheart/bdd-feature-runner-aws.git"
  },
  "keywords": [
    "nRFCloud",
    "IoT",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/coderbyheart/bdd-feature-runner-aws/issues"
  },
  "homepage": "https://github.com/coderbyheart/bdd-feature-runner-aws#readme",
  "dependencies": {
    "@types/backoff": "^2.5.1",
    "@types/chai": "^4.2.2",
    "@types/glob": "^7.1.1",
    "@types/graphql": "^14.5.0",
    "@types/jsonata": "^1.3.1",
    "@types/node-fetch": "^2.5.0",
    "aws-sdk": "^2.526.0",
    "backoff": "^2.5.0",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "fetch-ponyfill": "^6.1.0",
    "gherkin": "^5.1.0",
    "glob": "^7.1.4",
    "graphql": "^14.5.4",
    "jsonata": "^1.6.5",
    "node-fetch": "^2.6.0",
    "paho-mqtt": "^1.1.0",
    "toposort": "^2.0.2",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "@bifravst/code-style": "^3.0.1",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-angular": "^8.1.0",
    "@nrfcloud/tslint-config": "^3.10.0",
    "@types/chai-subset": "^1.3.3",
    "@types/node": "^12.7.5",
    "@types/paho-mqtt": "^1.0.4",
    "@types/ws": "^6.0.3",
    "husky": "^3.0.5"
  },
  "files": [
    "package-lock.json",
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "saga",
    "remoteTags": true
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts",
      "git add"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write",
      "git add"
    ]
  }
}
